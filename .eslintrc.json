{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "12"
    },
    "extends": [
        "prettier",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "plugins": ["prettier", "@typescript-eslint"],
    "rules": {
        "prettier/prettier": ["error"],
        "@typescript-eslint/no-unused-vars": "off"
    },

    "env": {
        "browser": true,
        "es2021": true
    }
}
